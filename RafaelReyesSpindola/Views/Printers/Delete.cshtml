@model RafaelReyesSpindola.Models.Printer

@{
    ViewData["Title"] = "Delete";
}

<h1>Eliminar</h1>

<h3>Ya no podrás imprimir tickets, ¿deseas continuar?</h3>
<div>
    <h4>Impresora</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nombre):
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nombre)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="ID" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a style="text-decoration:none;" asp-action="Index">Volver</a>
    </form>
</div>
